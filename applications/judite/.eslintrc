{
  "parser": "babel-eslint",
  "extends": [
    "prettier-standard"
  ],
  "plugins": [
    "prettier",
    "standard"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "2018"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}

